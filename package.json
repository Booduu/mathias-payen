{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "targets": {
    "frontend": {
      "source": [
        "src/app.js",
        "src/styles/*.scss"
      ]
    }
  },
  "scripts": {
    "backend:development": "nodemon ./bin/www",
    "frontend:build": "parcel build src/app.js && src/styles/*.scss ",
    "frontend:development": "parcel serve watch src/app.js && src/styles/*.scss",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\" --no-cache"
  },
  "dependencies": {
    "@prismicio/client": "^7.3.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "gsap": "^3.12.4",
    "http-errors": "~1.6.3",
    "include-media": "^2.0.0",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.10.1",
    "@parcel/transformer-sass": "^2.10.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "3.2.5"
  }
}
